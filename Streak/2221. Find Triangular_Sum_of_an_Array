class Solution{
    public int triangularSum(int[] nums){
        int[] temp = getIt(nums);
        return temp[0];
    }
    public int[] getIt(int[] a){
        if (a.length==1) return a;

        int[] b = new int[a.length-1];

        for(int i=0; i<a.length-1;i++){
            b[i] = (a[i] + a[i+1])%10;
        }
        return getIt(b);
    }
}
