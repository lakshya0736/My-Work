class Solution{
    public int maxCount(int m, int n, int[][] ops){
    int count = 0;
    int minA = m;
    int minB = n;
    for(int[] op: ops){
         minA=Math.min(minA,op[0]);
         minB=Math.min(minB,op[1]);
    }
    if(ops.length==0){
        return m*n;
    }
    count=minA*minB;
    return count;
    }
}
