class Solution {
    public int findContentChildren(int[] g, int[] s) {
        Arrays.sort(s);
        Arrays.sort(g);
        int children = 0;
        int cookie = 0;
        int n = s.length;
        int m = g.length;
        while(cookie<n&&children<m){
            if(s[cookie]>=g[children]){
                children++;
            }
            cookie++;
        }
        return children;
        
    }
}
