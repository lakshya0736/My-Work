class Solution {
    public void moveZeroes(int[] nums) {
        int n =nums.length;
        // int i=0;
        // for(int j=0; j<n; j++){
        //     if(nums[j]!=0){
        //         int temp = nums[i];
        //         nums[i]=nums[j];
        //         nums[j]=temp;
        //         i++;
        //     }
        // }
        int count = 0;
        for(int i=0; i<n; i++){
           if(nums[i]!=0){
            nums[count++]=nums[i];
           } 
        }
        while(count<n){
            nums[count++]=0;
        }
    }
}
